{
  "name": "storage-orm",
  "version": "0.1.2",
  "description": "a minified library to manage local storages (e.g. localStorage, sessionStorage, indexedDB etc.) just like a real database with more functionality",
  "main": "dist",
  "scripts": {
    "start": "webpack serve --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arashi-dev/local-orm.git"
  },
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/arashi-dev/local-orm#readme",
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage",
    "indexedDB",
    "orm",
    "database",
    "offline"
  ],
  "author": "arashi-dev",
  "license": "ISC",
  "devDependencies": {
    "@types/object-path": "^0.11.1",
    "compression-webpack-plugin": "^9.0.0",
    "html-webpack-plugin": "^5.3.2",
    "ts-essentials": "^9.0.0",
    "ts-loader": "^9.2.6",
    "type-fest": "^2.5.2",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "object-path": "^0.11.8"
  }
}
